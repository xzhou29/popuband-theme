<script>
  window.onload = function(){
    var element = document.getElementById("control-main");

    element.classList.remove("main-content");

    document.getElementById("features-group").style.visibility = "visible";

    
	document.getElementById("remove-bottom").style.height = "0";

  };
</script>


<script type="text/javascript">
  window.onerror = function(msg){
    var headEl = document.getElementsByTagName("head")[0];
    headEl.setAttribute("data-js-error",msg);
  }
</script>       
